<!DOCTYPE html>
<html>
<head>
  <title>Blogger</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body class="skin-blue layout-top-nav">
	<div class="wrapper">
		<%= render 'layouts/header' %>
		<div class="content-wrapper">
		<div class="container">
			<section class="content">
				<%= yield %>
			</section>
		</div>
	</div>
	<%= render 'layouts/footer' %>
</div>	
</body>
</html>
